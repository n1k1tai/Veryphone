		  			<h2>Inscription</h2>
		 			<form role="form">
		 				<div class="row">
		 					<div class="col-md-6">
				 				<div class="form-group">
				      				<label for="nom">Nom</label>
				      				<input type="text" class="form-control" id="nom" placeholder="Nom">
				    			</div>
				    		</div>	
